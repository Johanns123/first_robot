<?xml version="1.0"?>
<robot name="vehicle_blue">

    <gazebo>
        <plugin filename="libgazebo_ros_control.so" name="gazebo_ros_control">
            <robotNamespace>/rrbot</robotNamespace>
            <robotSimType>gazebo_ros_control/DefaultRobotHWSim</robotSimType>
        </plugin>
    </gazebo>
    
   <!-- Link1 -->
  <gazebo reference="link1">
    <material>Gazebo/Orange</material>
  </gazebo>
  <!-- Link2 -->
  <gazebo reference="link2">
    <mu1>0.2</mu1>
    <mu2>0.2</mu2>
    <material>Gazebo/Black</material>
  </gazebo>
  <!-- Link3 -->
  <gazebo reference="link3">
    <mu1>0.2</mu1>
    <mu2>0.2</mu2>
    <material>Gazebo/Orange</material>
  </gazebo>
  <!-- camera_link -->
  <gazebo reference="camera_link">
    <mu1>0.2</mu1>
    <mu2>0.2</mu2>
    <material>Gazebo/Red</material>
  </gazebo>
  

  <!-- Chassis -->
  <link name="chassis">
    <inertial>
      <mass value="1.14395"/>
      <inertia ixx="0.095329" ixy="0" ixz="0" iyy="0.381317" iyz="0" izz="0.476646"/>
    </inertial>
    <visual>
      <geometry>
        <box size="1.8 0.8 0.3"/>
      </geometry>
      <material>
        <color rgba="0.0 0.0 1.0 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="1.8 0.8 0.3"/>
      </geometry>
    </collision>
  </link>

  <!-- Lidar -->
  <joint name="lidar_joint" type="fixed">
    <parent link="chassis"/>
    <child link="lidar"/>
    <origin xyz="0.8 0 0.5" rpy="0 0 0"/>
  </joint>
  <link name="lidar">
    <sensor type="gpu_lidar" name="gpu_lidar">
      <update_rate value="10"/>
      <ray>
        <scan>
          <horizontal>
            <samples value="640"/>
            <min_angle value="-1.396263"/>
            <max_angle value="1.396263"/>
          </horizontal>
          <vertical>
            <samples value="1"/>
            <min_angle value="0"/>
            <max_angle value="0"/>
          </vertical>
        </scan>
        <range>
          <min value="0.08"/>
          <max value="10.0"/>
        </range>
      </ray>
    </sensor>
  </link>

  <!-- Left Wheel 1 -->
  <link name="left_wheel1">
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.043333" ixy="0" ixz="0" iyy="0.043333" iyz="0" izz="0.08"/>
    </inertial>
    <visual>
      <geometry>
        <cylinder radius="0.15" length="0.10"/>
      </geometry>
      <material>
        <color rgba="1.0 0.0 0.0 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.15" length="0.10"/>
      </geometry>
    </collision>
  </link>

  <!-- Right Wheel 1 -->
  <link name="right_wheel1">
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.043333" ixy="0" ixz="0" iyy="0.043333" iyz="0" izz="0.08"/>
    </inertial>
    <visual>
      <geometry>
        <cylinder radius="0.15" length="0.10"/>
      </geometry>
      <material>
        <color rgba="1.0 0.0 0.0 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.15" length="0.10"/>
      </geometry>
    </collision>
  </link>

  <!-- Left Wheel 2 -->
  <link name="left_wheel2">
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.043333" ixy="0" ixz="0" iyy="0.043333" iyz="0" izz="0.08"/>
    </inertial>
    <visual>
      <geometry>
        <cylinder radius="0.15" length="0.10"/>
      </geometry>
      <material>
        <color rgba="1.0 0.0 0.0 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.15" length="0.10"/>
      </geometry>
    </collision>
  </link>

  <!-- Right Wheel 2 -->
  <link name="right_wheel2">
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.043333" ixy="0" ixz="0" iyy="0.043333" iyz="0" izz="0.08"/>
    </inertial>
    <visual>
      <geometry>
        <cylinder radius="0.15" length="0.10"/>
      </geometry>
      <material>
        <color rgba="1.0 0.0 0.0 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.15" length="0.10"/>
      </geometry>
    </collision>
  </link>

  <!-- Joints -->
  <joint name="left_wheel1_joint" type="revolute">
    <parent link="chassis"/>
    <child link="left_wheel1"/>
    <origin xyz="-0.4 0.46 -0.05" rpy="0 -1.5707 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <joint name="right_wheel1_joint" type="revolute">
    <parent link="chassis"/>
    <child link="right_wheel1"/>
    <origin xyz="-0.4 -0.46 -0.05" rpy="0 -1.5707 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <joint name="left_wheel2_joint" type="revolute">
    <parent link="chassis"/>
    <child link="left_wheel2"/>
    <origin xyz="0.4 0.46 -0.05" rpy="0 -1.5707 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <joint name="right_wheel2_joint" type="revolute">
    <parent link="chassis"/>
    <child link="right_wheel2"/>
    <origin xyz="0.4 -0.46 -0.05" rpy="0 -1.5707 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <!-- Diff Drive Plugin -->
  <gazebo>
    <plugin name="diff_drive" filename="libgazebo_ros_diff_drive.so">
      <left_joint>left_wheel1_joint</left_joint>
      <left_joint>left_wheel2_joint</left_joint>
      <right_joint>right_wheel1_joint</right_joint>
      <right_joint>right_wheel2_joint</right_joint>
      <wheel_separation>0.92</wheel_separation>
      <wheel_radius>0.15</wheel_radius>
      <update_rate>50</update_rate>
      <robot_namespace>/</robot_namespace>
      <command_topic>cmd_vel</command_topic>
      <odometry_topic>odom</odometry_topic>
      <odometry_frame>odom</odometry_frame>
      <odometry_publisher_rate>50</odometry_publisher_rate>
    </plugin>
  </gazebo>
</robot>
